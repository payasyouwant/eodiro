    "css-element-queries": "^1.2.3",
    "dayjs": "^1.10.6",
    "escape-html": "^1.0.3",
    "immer": "^9.0.5",
    "intersection-observer": "^0.12.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "next": "^11.1.0",
    "pluralize": "^8.0.0",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-if": "^4.0.1",
    "recoil": "0.3.1",
    "socket.io-client": "^4.1.3",
